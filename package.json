{
    "name": "lugo-bot-typescript",
    "version": "1.0.0",
    "description": "",
    "keywords": [],
    "author": "",
    "license": "ISC",
    "main": "dist/main.js",
    "scripts": {
        "start": "node dist/main.js",
        "play": "clear && tsc && node --env-file .env dist/main.js",
        "lint:check": "eslint 'src/**'",
        "lint:fix": "npm run lint -- --fix",
        "style:check": "prettier --check 'src/**/*.ts'",
        "style:fix": "prettier --write 'src/**/*.ts'",
        "watcher:up": "docker compose -f watcher-compose.yml -p builder up -d",
        "watcher:down": "docker compose -f watcher-compose.yml -p builder down"
    },
    "dependencies": {
        "@lugobots/lugo4node": "^1.0.1-beta.1"
    },
    "devDependencies": {
        "@types/node": "^20.4.9",
        "@typescript-eslint/eslint-plugin": "^5.62.0",
        "@typescript-eslint/parser": "^5.59.6",
        "eslint": "^8.46.0",
        "eslint-config-prettier": "^9.0.0",
        "eslint-plugin-prettier": "^5.0.0",
        "prettier": "^3.0.1",
        "tsc-watch": "^5.0.3",
        "typescript": "^4.9.5"
    }
}
